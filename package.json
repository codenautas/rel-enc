{
  "name": "rel-enc",
  "description": "Relativo a encuestas",
  "version": "1.2.4",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "main": "dist/server/rel-enc.js",
  "types": "dist/server/rel-enc.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "operativos": "^1.1.6",
    "row-validator": "^0.6.1"
  },
  "devDependencies": {
    "types.d.ts": "~0.5.0",
    "mixin-patch": "~0.3.3"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "//prebuild:force": "(tsc -p src/client & tsc -p src/server || echo \"continue w/error\")",
    "//build:force": "npm run //prebuild:force && mixin-patch",
    "prepare": "npm run build",
    "prebuild": "tsc -b src/server src/client",
    "build": "npm run prebuild && mixin-patch",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node dist/server/server-rel-enc.js",
    "git-commit": "git add package.json && (git commit -m \"upgrading stack dependencies\" || true)",
    "push-publish": "npm version patch && ncu -u && npm run git-commit && npm publish && git push"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "lib",
    "ecmaVersion": 6
  }
}
