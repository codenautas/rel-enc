{
  "name": "rel-enc",
  "description": "Relativo a encuestas",
  "version": "0.3.0",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "main": "server/rel-enc.js",
  "types": "server/rel-enc.d.ts",
  "files": [
    "client",
    "server",
    "src"
  ],
  "dependencies": {
    "operativos": "~0.2.4"
  },
  "devDependencies": {
    "types.d.ts": "~0.1.32"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "(tsc -p tsconfig-client.json || echo \"continue w/error\") && (tsc -p tsconfig-server.json  || echo \"continue w/error\")",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node server/server-rel-enc.js"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "lib",
    "ecmaVersion": 6
  }
}
